{
  "name": "ScalerBot",
  "version": "1.0.0",
  "private": true,
  "description": "Utility bot for Scaler's server",
  "author": "Alex",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "shx rm -rf ./dist/ && tsc --project tsconfig.json",
    "start": "npm run build && cross-env DOTENV_CONFIG_PATH=./.env.production ts-node -r dotenv/config dist/index.js",
    "dev": "npm run build && cross-env DOTENV_CONFIG_PATH=./.env.dev ts-node -r dotenv/config dist/index.js"
  },
  "dependencies": {
    "@sapphire/framework": "^5.3.6",
    "@sapphire/plugin-subcommands": "^7.0.1",
    "@sapphire/utilities": "^3.18.2",
    "axios": "^1.11.0",
    "discord-api-types": "^0.38.11",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "mongoose": "^8.15.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "shx": "^0.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CommandLineFox/NotificationBot.git"
  },
  "bugs": {
    "url": "https://github.com/CommandLineFox/NotificationBot/issues"
  },
  "homepage": "https://github.com/CommandLineFox/NotificationBot#readme"
}
